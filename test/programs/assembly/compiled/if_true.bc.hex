section .text
  jmp our_code_starts_here
our_code_starts_here:
  mov eax, 0
  mov esp, 128
  mov ebp, 128
  push ebp
  mov ebp, esp
  add esp, 0
  mov eax, 0xffffffff
  and eax, 0x7fffffff
  cmp eax, 0x7fffffff
  jne err_if_not_bool
  mov eax, 0xffffffff
  cmp eax, 0xffffffff
